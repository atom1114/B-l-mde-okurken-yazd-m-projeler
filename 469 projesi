def sayilari_isle(metin):
    try:
        # Metni satır satır ayırıp integer sayılara çevirme
        sayilar = [int(sayi) for sayi in metin.strip().split('\n') if sayi.strip()]

        # Sayıları listeleme
        print("\nSayılar:")
        for i, sayi in enumerate(sayilar, 1):
            print(f"{i}. {sayi}")

        # İstatistikleri hesaplama
        toplam = sum(sayilar)
        sayi_adedi = len(sayilar)
        bolum = toplam / 64000000  # 400x400x400
        yuzde = bolum * 100

        # Sonuçları yazdırma
        print("\nİstatistikler:")
        print(f"Toplam Sayı Adedi: {sayi_adedi}")
        print(f"Sayıların Toplamı: {toplam:,}")
        print(f"64.000.000'e bölümü: {bolum:.6f}")
        print(f"Yüzde olarak: %{yuzde:.2f}")

    except ValueError as e:
        print(f"Hata: Sayıya çevrilemeyen değer bulundu - {e}")
    except Exception as e:
        print(f"Beklenmeyen bir hata oluştu: {e}")

# Fonksiyonu çağır
sayilari_isle(sayilar_text)
