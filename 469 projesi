sayilar_text = """584,329,251,448,2032,2287,2429,1413,124,1405,558,2645,1114,516,2605,1179,7469,604,398,718,417,49,43,2234,697,951,2063,2334,692,145,434,141,1153,675,938,922,814,1900,610,525,12,2900,3092,1310,106,246,157,1990,6047,1186,455,501,763,603,863,10905,1139,2429,515,802,405,449,216,928,608,1534,4555,2303,253,1214,448,636,101,30,937,1460,1362,136,1177,331,1198,685,2332,1214,479,908,822,791,811,177,1567,477,628,1752,143,862,327,825,5254,148,1681,715,167,678,375,3251,909,476,481,634,4939,310,1976,295,975,464,265,643,1193,3710,407,970,1335,739,2850,222,1445,623,481,196,279,1207,1509,65,68,829,397,569,696,1640,380,5229,1847,1270,534,1235,1094,225,1862,363,154,139,1251,901,1259,1989,534,6721,342,438,1818,628,1984,2903,1055,2662,363,1368,1739,1582,264,2721,816,2573,1251,366,1182,138,650,672,1129,93,522,33,2545,1799,2395,283,965,2365,230,1353,909,432,227,168,160"""

sayilar = [int(sayi) for sayi in sayilar_text.split(',')]

print("Sayılar:")
for i, sayi in enumerate(sayilar, 1):
    print(f"{i}. {sayi}")

toplam = sum(sayilar)

print("\nToplam Sayı:", len(sayilar))
print("Toplam:", toplam)

# 64000000'e bölelim 400x400x400 hacim olduğu için
bolum = toplam / 64000000


yuzde = bolum * 100

print("64000000'e bölümü:", bolum)
print("Yüzde olarak: %{:.2f}".format(yuzde))

